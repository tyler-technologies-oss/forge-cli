{
  "root": true,
  "ignorePatterns": [
    "dist/**/*",
    "src/demo/**/*",
    "*.spec.ts"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "parserOptions": {
        "project": [
          "tsconfig.json"
        ],
        "tsconfigRootDir": ".",
        "createDefaultProgram": true
      },
      "extends": [
        "plugin:@tylertech-eslint/recommended"
      ],
      "rules": {
        "@typescript-eslint/no-empty-interface": "off"
      }
    }
  ]
}
